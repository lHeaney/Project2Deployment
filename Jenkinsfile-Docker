pipeline {
    agent any
    stages {
        stage ('build Docker Images')
        {
            steps{
                //this is for comments
            
            sh '''
                echo "Build Image: aws-cli"
                docker build -f ./Dockerfile -t aws-cli .

                #future database for docker build
                echo "Building Docker Image $DOCKER_IMAGE"
            '''
            }
        }
    }
}